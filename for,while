// renata.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

void main ()

{
    int I,N ;
	cout<<"Ingrese N: ";
	cin>>N;
	for (I=1;I<=N;I++)
		cout<<I<< "\t";
	I=2;
	cout<<"\n";
    while(I<=N)
	{ 
		cout<<I<<"\t";
		I=I+2;
	}
	
getch();
}
